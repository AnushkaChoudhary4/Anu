        "import random\n",
        "def guess_the_no():\n",
        "  no=random.randint(1,1000)\n",
        "  while True:\n",
        "   user_guess=int(input(\"enter\"))\n",
        "   if user_guess==no:\n",
        "    print(\"equal to number\")\n",
        "    break\n",
        "   elif user_guess<no:\n",
        "    print(\"smaller then mumber\")\n",
        "   else:\n",
        "    print(\"greater then number\")\n",
        "  guess_the_no(\"betweeen 1 to 1000\")"
      ]
    }
